# Server Configuration
PORT=3001
NODE_ENV=development

# Odoo Backend Configuration
ODOO_BASE_URL=
ODOO_DATABASE=p2p
ODOO_USERNAME=admin
ODOO_PASSWORD=admin

# API Configuration
API_PREFIX=/api/v1
CACHE_TTL=300
REQUEST_TIMEOUT=15000

# Security
JWT_SECRET=your-super-secret-jwt-key-change-in-production
API_RATE_LIMIT_WINDOW=900000
API_RATE_LIMIT_MAX=100

# Logging
LOG_LEVEL=info