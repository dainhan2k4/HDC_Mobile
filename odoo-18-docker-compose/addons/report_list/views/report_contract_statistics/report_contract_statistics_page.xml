<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <template id="report_list.report_contract_statistics_page_template" name="Report Contract Statistics Page">
        <t t-call="web.layout">
            <t t-set="title">Báo cáo thống kê HĐ theo kỳ hạn</t>
            <t t-call-assets="web.assets_frontend" t-js="true"/>
            <t t-set="head">
                <!-- Dependencies -->
                <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css" rel="stylesheet"/>
                <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/js/bootstrap.bundle.min.js"></script>
                <link href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.4.0/css/all.min.css" rel="stylesheet"/>
                
                <!-- OWL Framework -->
                <script src="https://cdnjs.cloudflare.com/ajax/libs/owl-framework/2.0.0/owl.min.js"></script>
                
                <!-- Custom CSS -->
                <link rel="stylesheet" type="text/css" href="/report_list/static/src/css/report-list.css"/>
                
 
                
            </t>
            <!-- Header Container for Management -->
            <div id="headermana-container"></div>
            <div class="app-container">
                <!-- Mobile Overlay -->
                <div class="sidebar-overlay" id="sidebarOverlay"></div>
                
                <!-- Main Content -->
                <main class="main-content">
                    <div class="container-fluid">
                        <div id="reportContractStatisticsWidget"></div>
                    </div>
                </main>
            </div>
        </t>
    </template>
</odoo>
