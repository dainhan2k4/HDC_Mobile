<odoo>
    <template id="country_list" name="Danh mục Quốc gia">
        <t t-call="web.layout">
            <t t-set="title">Danh mục Quốc gia</t>
            <t t-call-assets="web.assets_backend" t-js="true"/>
            <t t-set="head">
                <!-- Bootstrap 5 CSS -->
                <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css" rel="stylesheet" xintegrity="sha384-QWTKZyjpPEjISv5WaRU9OFeRpok6YctnYmDr5pNlyT2bRjXh0JMhjY6hW+ALEwIH" crossorigin="anonymous"/>
                <!-- Bootstrap 5 JS Bundle -->
                <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/js/bootstrap.bundle.min.js" xintegrity="sha384-YvpcrYf0tY3lHB60NNkmXc5s9fDVZLESaAA55NDzOxhy9GkcIdslK1eN7N6jIeHz" crossorigin="anonymous"></script>
                <!-- Font Awesome for icons -->
                <link href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.4.0/css/all.min.css" rel="stylesheet"/>
                <style>
                    body {
                        background-color: #f8f9fa;
                    }
                </style>
            </t>
            <div class="d-flex">
                <t t-call="fund_management_control.sidebar_template"/>
                <main class="flex-grow-1 p-4">
                    <div class="container-fluid">
                        <div class="d-flex justify-content-between align-items-center mb-4">
                            <h2 class="h3 fw-bold mb-0">Danh mục Quốc gia</h2>
                        </div>
                        <div class="card shadow-sm">
                            <div class="card-body">
                                <div id="countryWidget"></div>
                            </div>
                        </div>
                    </div>
                </main>
            </div>
        </t>
    </template>
</odoo> 
